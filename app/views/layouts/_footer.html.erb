<footer>
  <small>
    <% if current_user %>
      Signed in as <%= current_user.full_name %> (<%= link_to "Log out", session_path("current"), method: :delete %>)
    <% else %>
      <%= link_to "Log In", new_session_path %> | <%= link_to "Sign Up", new_user_path %>
    <% end %>
    <% if user_admin? || session[:admin_id] %>
      <%= link_to "view all users", admin_users_path %>
      <%= link_to "Create new users", new_admin_user_path %>
      <%= link_to "Log back into admin", admin_login_back_to_admin_path %>
    <% end %>
  </small>
</footer>